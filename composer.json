{
  "name": "byjg/serializer",
  "description": "Serialize any object into array and format it JSON or XML",
  "autoload": {
    "psr-4": {
      "ByJG\\Serializer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6",
    "ext-json": "*",
    "symfony/yaml": "> 4"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|7.4.*|^9.5"
  },
  "suggest": {
    "ext-curl": "*"
  },
  "license": "MIT"
}
