{
  "name": "byjg/serializer",
  "description": "Serialize any object into array and format it JSON or XML",
  "autoload": {
    "psr-4": {
      "ByJG\\Serializer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1 <8.5",
    "ext-json": "*",
    "ext-simplexml": "*",
    "symfony/yaml": "> 4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6|^10.5|^11.5",
    "vimeo/psalm": "^6.0"
  },
  "suggest": {
    "ext-curl": "*"
  },
  "license": "MIT"
}
